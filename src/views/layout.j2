{% set tabletWidth = "768px"%}
{% set desktopWidth = "1181px"%}

<!DOCTYPE html>
<html class="page" lang="ru">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ site_name }}</title>
  {# <link rel="icon" href="favicon.ico"> #}
  <link rel="preload" as="style" href="css/style.min.css" >
  <link rel="stylesheet" href="css/style.min.css">
  <script src="./js/global.min.js" defer></script>
  <script src="./js/burger.min.js" defer></script>
  <script src="./js/pristine.min.js" defer></script>
  <script src="./js/choices.min.js" defer></script>
  <script src="./js/imask.js" defer></script>
  <script src="./js/modal.min.js" defer></script>
  <script src="./js/menu.min.js" defer></script>
  {% block js %}{% endblock %}
</head>
<body class="page__body">
  {% include "header.j2" %}

  {% block content %}{% endblock %}

  {% include "footer.j2" %}


  {% include "./blocks/modal.j2" %}
  {% include "./blocks/modal-order.j2" %}
  {% include "./blocks/modal-offer.j2" %}
  {% include "./blocks/modal-consultation.j2" %}
  {% include "./blocks/modal-pricelist.j2" %}
  {% block modal %}{% endblock %}


</body>
</html>
